{
	"publisher": "jsdoc",
	"name": "jsdoc",
	"displayName": "jsdoc",
	"description": "gen jsdoc template",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [
		"onCommand:extension.createJSDoc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createJSDoc",
				"title": "根据选中函数的参数生成一个 jsdoc 风格的注释模板"
			}
		],
		"configuration": [
			{
				"title": "jsdoc",
				"properties": {
					"jsdoc.author": {
						"type": "string",
						"default": "",
						"description": "添加此选项, jsdoc 将自动插入作者"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
